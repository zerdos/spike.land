{
  "name": "@spike-npm-land/transpile",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.9.56",
  "type": "module",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "dev": "wrangler dev",
    "dev:remote": "wrangler dev --remote",
    "// build": "yarn clean",
    "build:worker": "esbuild src/index.ts --bundle --outfile=dist/index.js --platform=browser --format=esm",
    "prebuild": "cp -r ../code/dist ./node_modules/@spike-npm-land/code/dist",
    "build": "yarn prebuild && yarn build:worker",
    "w:deploy:prod": "wrangler deploy src/index.ts",
    "clean": "rimraf dist",
    "types:check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "devDependencies": {
    "@cloudflare/kv-asset-handler": "0.3.4",
    "rimraf": "6.0.1",
    "typescript": "5.7.3",
    "wrangler": "3.109.2"
  },
  "dependencies": {
    "@spike-npm-land/code": "0.9.56",
    "esbuild-wasm": "0.25.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
