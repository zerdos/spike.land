{
  "compilerOptions": {
    "allowJs": true,
    "alwaysStrict": true,
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noEmit": false,
    "forceConsistentCasingInFileNames": true,
    "preserveConstEnums": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "target": "ES2022",
    "module": "CommonJS",
    "lib": ["ES2020", "ES2020.String", "ES2015", "es2021", "ES2022.Error"],
    "types": ["@cloudflare/workers-types"]
  },
  "include": [
    "src/*"
  ]
}
