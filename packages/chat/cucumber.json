{
  "default": {
    "paths": ["features/**/*.feature"],
    "require-module": ["tsx"],
    "require": ["features/**/*.ts"],
    "format": ["@cucumber/pretty-formatter"],
    "formatOptions": {
      "snippetInterface": "async-await"
    },
    "publishQuiet": true,
    "parallel": 1,
    "retry": 1,
    "retryTagFilter": "@flaky"
  }
}