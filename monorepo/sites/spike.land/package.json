{
  "name": "@spike.land/code-worker",
  "description": "An edge chat service that runs on Cloudflare Workers using Durable Objects",
  "version": "0.7.79",
  "main": "dist/index.mjs",
  "type": "module",
  "module": "dist/index.mjs",
  "private": true,
  "scripts": {
    "dev": "miniflare -m --wrangler-config wrangler.toml --https --host dev.spike.land --port 8080",
    "deploy": "yarn xbuild && wrangler publish src/index.ts",
    "xbuild": "esbuild --bundle --format=esm --loader:.html=text --target=esnext src/index.ts --outfile=dist/index.mjs"
  },
  "devDependencies": {
    "@spike.land/cf-npm-site": "^0.7.70",
    "@spike.land/code": "^0.7.79"
  },
  "dependencies": {
    "@types/websocket": "1.0.5",
    "cookie": "0.4.2",
    "miniflare": "2.3.0",
    "textdiff-patch": "1.0.6"
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "db8e23b83d2dd82efc101eca9d1b5ec76d122df4"
}
