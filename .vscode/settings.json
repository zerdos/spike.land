{
  "testing.alwaysRevealTestOnStateChange": false,
  "files.refactoring.autoSave": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/.pnp.*": true,
    "**/.yarn": true
  },
  "search.useIgnoreFiles": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "git.autorefresh": true,
  "git.autofetch": true,
  "npm.packageManager": "yarn",
  "explorer.expandSingleFolderWorkspaces": true,
  "cSpell.words": [
    "applewebkit",
    "arcanis",
    "autoremove",
    "axax",
    "bmflb",
    "checkpointer",
    "codicon",
    "dearmor",
    "devcontainers",
    "devimages",
    "diffpatcher",
    "dockerhub",
    "dpkg",
    "edcore",
    "Embeddable",
    "Erdos",
    "esbuild-wasm",
    "esms",
    "filecache",
    "Helia",
    "Hibernatable",
    "importmap",
    "ipfs",
    "jsondiffpatch",
    "jsximportsource",
    "langraph",
    "libxtst",
    "lucide",
    "Memoizer",
    "metafile",
    "Miniflare",
    "nums",
    "onperiodicsync",
    "outfile",
    "pflannery",
    "preact",
    "pseudocode",
    "roleplay",
    "Saionaro",
    "scrollback",
    "Sess",
    "shadcn",
    "snakecase",
    "spikeland",
    "stunprotocol",
    "subword",
    "tailwindcss",
    "tanstack",
    "testspace",
    "thisp",
    "USEPOLLING",
    "vaul",
    "versionlens",
    "zipfs",
    "Zoltan"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/dist/**": true,
    "**/dist-vite/**": true,
    "**/build/**": true,
    "**/.cache/**": true
  },
  "typescript.disableDirectoryWatching": [
    "**/dist/**",
    "**/dist-vite/**",
    "**/build/**",
    "**/.cache/**",
    "**/node_modules/**"
  ],
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.maxTsServerMemory": 4096,
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriority"
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.log": "off",
  "eslint.nodePath": ".yarn/sdks",
  "editor.tabCompletion": "on",
  "diffEditor.codeLens": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "MutableAI.upsell": false,
  "typescript.tsdk": ".yarn/sdks/typescript/lib",
  "vitest.disableWorkspaceWarning": true
}
