{
  "testing.alwaysRevealTestOnStateChange": false,
  "files.refactoring.autoSave": true,
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/dist": true,
    "**/.pnp.*": true,
    "**/.yarn": true
  },
  "search.useIgnoreFiles": true,
  "files.autoSave": "afterDelay",
  "files.autoSaveDelay": 300,
  "git.autorefresh": true,
  "git.autofetch": true,
  "npm.packageManager": "yarn",
  "explorer.expandSingleFolderWorkspaces": true,
  "cSpell.words": [
    "applewebkit",
    "arcanis",
    "asynciterable",
    "autoremove",
    "axax",
    "bmflb",
    "checkpointer",
    "codicon",
    "dearmor",
    "devcontainers",
    "devimages",
    "diffpatcher",
    "dockerhub",
    "dpkg",
    "edcore",
    "Embeddable",
    "Erdos",
    "esbuild-wasm",
    "esms",
    "filecache",
    "Helia",
    "Hibernatable",
    "importmap",
    "ipfs",
    "jsondiffpatch",
    "jsximportsource",
    "langraph",
    "libxtst",
    "lucide",
    "Memoizer",
    "metafile",
    "Millis",
    "Miniflare",
    "nums",
    "nuxt",
    "onperiodicsync",
    "outfile",
    "pflannery",
    "preact",
    "pseudocode",
    "roleplay",
    "Saionaro",
    "scrollback",
    "Sess",
    "shadcn",
    "snakecase",
    "spikeland",
    "stunprotocol",
    "subword",
    "tailwindcss",
    "tanstack",
    "testspace",
    "thisp",
    "USEPOLLING",
    "vaul",
    "vercel",
    "versionlens",
    "zipfs",
    "Zoltan"
  ],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true,
    "**/dist/**": true,
    "**/dist-vite/**": true,
    "**/build/**": true,
    "**/.cache/**": true
  },
  "typescript.disableDirectoryWatching": [
    "**/dist/**",
    "**/dist-vite/**",
    "**/build/**",
    "**/.cache/**",
    "**/node_modules/**"
  ],
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.tsserver.maxTsServerMemory": 8192,
  "typescript.tsserver.watchOptions": {
    "watchFile": "useFsEvents",
    "watchDirectory": "useFsEvents",
    "fallbackPolling": "dynamicPriorityPolling",
    "excludeDirectories": [
      "**/node_modules",
      "**/dist",
      "**/dist-vite",
      "**/.yarn",
      "**/build"
    ]
  },
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "typescript.tsserver.log": "off",
  "typescript.tsserver.useSyntaxServer": "auto",
  "typescript.tsserver.experimental.useVsCodeWatcher": true,
  "eslint.nodePath": ".yarn/sdks",
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "eslint.runtime": "node",
  "eslint.codeActionsOnSave.mode": "problems",
  "eslint.run": "onType",
  "eslint.trace.server": "off",
  "editor.tabCompletion": "on",
  "diffEditor.codeLens": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "css.customData": [
    ".vscode/tailwind.json"
  ],
  "MutableAI.upsell": false,
  "typescript.tsdk": "node_modules/typescript/lib",
  "vitest.disableWorkspaceWarning": true,
  "typescript.preferences.includePackageJsonAutoImports": "auto",
  "javascript.preferences.includePackageJsonAutoImports": "auto",
  "typescript.workspaceSymbols.scope": "currentProject",
  "typescript.tsserver.experimental.useInlayHints": true,
  "javascript.validate.enable": true,
  "typescript.validate.enable": true,
  "typescript.suggest.completeJSDocs": true,
  "javascript.suggest.completeJSDocs": true,
  "search.followSymlinks": false,
  "prettier.prettierPath": ".yarn/sdks/prettier/index.cjs",
  "typescript.tsserver.experimental.enablePackageAutoImports": true,
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.preferences.quoteStyle": "double",
  "javascript.preferences.importModuleSpecifier": "relative",
  "javascript.preferences.quoteStyle": "double"
}
