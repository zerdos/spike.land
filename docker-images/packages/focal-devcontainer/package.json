{
  "name": "@devcontainer/focal-devcontainer",
  "tagname": "focal-devcontainer",
  "scripts": {
    "tag:version": "node -e 'const p = require(__dirname + `/package.json`); console.log(`devimages/${p.tagname}:${p.version}`)'",
    "tag:latest": "node -e 'const p = require(__dirname + `/package.json`); console.log(`devimages/${p.tagname}:latest`)'",
    "docker:push": "docker push $(yarn tag:version) && docker push $(yarn tag:latest)",
    "docker:build": "docker build . -t $(yarn tag:version) -t $(yarn tag:latest)",
    "build": "node index.js"
  },
  "files": [
    ".devcontainer/**/*",
    "docker-compose.yml",
    "Dockerfile",
    "README.md"
  ],
  "devDependencies": {
    "@devcontainer/generator": "^12.0.6"
  },
  "version": "12.0.6",
  "description": "devcontainer vscode docker xfce remoteDesktop",
  "main": "index.js",
  "repository": "git@github.com:zed-vision/monorepo.git",
  "author": "Zoltan Erdos <zolika84@gmail.com>",
  "license": "BSD-3-Clause",
  "gitHead": "af54e6ff16c9c20b6fba6365c211999fedeae2aa",
  "publishConfig": {
    "access": "public"
  }
}
