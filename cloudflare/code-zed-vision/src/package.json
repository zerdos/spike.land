{
    "name": "@zedvision/cf-worker",
    "version": "11.10.10",
    "description": "Worker script for zed.vision",
    "main": "dist/worker-script.min.js",
    "files": [
        "**/*"
    ],
    "scripts": {
        "dev": "yarn xbuild && wrangler preview",
        "clean": "rm -rf dist && mkdir dist && deno fmt src",
        "xebuild": "esbuild src/worker-script.js  --minify --outfile=dist/worker-script.min.js",
        "gbuild": "deno bundle --unstable ./src/index.ts dist/worker-script.js --no-check && yarn min",
        "min": "deno fmt dist && esbuild dist/worker-script.js  --minify --outfile=dist/worker-script.min.js",
        "deploy": "yarn build && wrangler publish"
    },
    "keywords": [
        "monaco",
        "editor",
        "react",
        "html",
        "vscode"
    ],
    "authors": [
        "Zoltan Erdos <zolika84@gmail.com>"
    ],
    "bugs": {
        "url": "https://github.com/zed-vision/monorepo"
    },
    "homepage": "https://zed.vision",
    "repository": "https://github.com/zed-vision/monorepo",
    "devDependencies": {
        "@cloudflare/workers-types": "2.1.0",
        "@cloudflare/wrangler": "1.13.0",
        "@zedvision/ipfs": "^11.10.10",
        "esbuild": "0.8.36"
    },
    "gitHead": "af54e6ff16c9c20b6fba6365c211999fedeae2aa",
    "author": "Zoltan Erdos <zolika84@gmail.com>",
    "license": "BSD-3-Clause",
    "publishConfig": {
        "access": "public"
    },
    "dependencies": {
        "uuid": "8.3.2"
    }
}
